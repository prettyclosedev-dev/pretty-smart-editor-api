{
  "name": "pretty-smart-editor-api",
  "private": true,
  "version": "1.0.0",
  "license": "MIT",
  "dependencies": {
    "@apollo/gateway": "2.0.5",
    "@mercurialweb/nexus-plugin-prisma": "0.40.1",
    "@prisma/client": "4.5.0",
    "apollo-server": "2.19.1",
    "apollo-server-core": "3.10.2",
    "apollo-server-express": "3.10.2",
    "axios": "0.27.2",
    "bcryptjs": "2.4.3",
    "dotenv": "16.0.1",
    "express": "4.18.1",
    "firebase-admin": "10.3.0",
    "graphql": "15.4.0",
    "graphql-middleware": "6.0.0",
    "graphql-shield": "7.5.0",
    "http": "0.0.1-security",
    "https": "1.0.0",
    "jsonwebtoken": "8.5.1",
    "moment": "2.29.4",
    "nexus": "1.3.0",
    "polotno": "^1.4.0"
  },
  "scripts": {
    "start": "node dist/server",
    "clean": "rm -rf dist",
    "build": "npm -s run clean && npm -s run generate && tsc",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "migrate-save:prisma": "prisma migrate save --experimental",
    "migrate-up:prisma": "prisma migrate up --experimental",
    "generate:prisma": "prisma generate",
    "studio:prisma": "prisma studio",
    "generate:nexus": "ts-node --transpile-only src/schema",
    "dev": "ts-node-dev --no-notify --respawn --transpile-only src/server"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.2",
    "@types/jsonwebtoken": "8.5.0",
    "@types/node": "12.19.11",
    "@types/ws": "7.4.0",
    "prisma": "4.5.0",
    "prisma-upgrade": "0.0.44",
    "ts-node": "9.1.1",
    "ts-node-dev": "1.1.1",
    "typescript": "4.1.3"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
